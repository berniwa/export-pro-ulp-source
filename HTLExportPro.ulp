void dialogAddEditBomPreset(int set){
	if (set==0){
		dlgMessageBox(lanGetText("dlg.cantEdit"));
		return;
	}

	string name = (set==bomPreCount)?lanGetText("general.newPreset"):bomPreNames[set];
	real offX = (set==bomPreCount)?bom_offsetX:bomPreOffX[set];
	real offY = (set==bomPreCount)?bom_offsetY:bomPreOffY[set];
	real width = (set==bomPreCount)?bom_width:bomPreWidth[set];

	dlgDialog(lanGetText("button."+(set==bomPreCount)?"add":"edit")) {
		dlgVBoxLayout{
			dlgHBoxLayout {
				dlgLabel(lanGetText("general.name"));
				dlgStretch(1);
				dlgStringEdit(name);
			}
			dlgHBoxLayout {
				dlgLabel(lanGetText("settings.offset.x"));
				dlgStretch(1);
				dlgRealEdit(offX);
				dlgLabel("mm");
			}
			dlgHBoxLayout {
				dlgLabel(lanGetText("settings.offset.y"));
				dlgStretch(1);
				dlgRealEdit(offY);
				dlgLabel("mm");
			}
			dlgHBoxLayout {
				dlgLabel(lanGetText("settings.bom.width"));
				dlgStretch(1);
				dlgRealEdit(width);
				dlgLabel("mm");
			}

			dlgHBoxLayout {
				dlgPushButton(lanGetText("button.save")){
					bomPreNames[set] = name;
					bomPreOffX[set] = offX;
					bomPreOffY[set] = offY;
					bomPreWidth[set] = width;
					if(set==bomPreCount) {
						bomPreset=set;
						bomPreCount++;
					}
					dlgAccept();
				};
				dlgPushButton(lanGetText("button.cancel")){
					dlgAccept();
				};
			}
		}
	};
}

