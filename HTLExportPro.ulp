		ExportSetup[row++] = "L15 	;-l15 			;15 17 18 20 " + mt + "			;" + layer_image + ";" + layer_pdf + "; Copper L15";
		ExportSetup[row++] = "L16	;-l16-bot		;16 17 18 20 " + mb + "			;" + layer_image + ";" + layer_pdf + "; Copper L16 - Bottom";
