		ExportSetup[row++] = "L7 	;-l07			;7 17 18 20 " + mt + "			;" + layer_image + ";" + layer_pdf + "; Copper L7";
		ExportSetup[row++] = "L8 	;-l08			;8 17 18 20 " + mt + "			;" + layer_image + ";" + layer_pdf + "; Copper L8";
