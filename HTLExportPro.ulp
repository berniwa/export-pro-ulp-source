string getEAGLEconfig(string config){
	config += " = ";

	string path=EAGLE_HOME;
	if(strstr(OS_SIGNATURE,"Windows")>=0){
		path=filedir(path);

		path += "/AppData/Roaming";

		if(EAGLE_VERSION<9){
			path+="/CadSoft/EAGLE/eaglerc.usr";
		}
		else{
			string sig[];
			strsplit(sig,EAGLE_SIGNATURE,' ');

			path+="/Eagle/settings/"+sig[2]+"/eaglerc";
		}
	}
	else if(strstr(OS_SIGNATURE,"Linux")>=0){
		if(EAGLE_VERSION<9){
			path +="/.eaglerc";
		}
		else{
			string sig[];
			strsplit(sig,EAGLE_SIGNATURE,' ');

			path+="/.local/share/Eagle/settings/"+sig[2]+"/eaglerc";
		}
	}
	else {
		debugMsg("hello MAC User");
		return "1";
	}
	string data[];
	int lines = fileread(data, path);

	for(int i=0;i<lines;i++) {
		if(strstr(data[i],config)!=-1) {
			int start = strchr(data[i],'"')+1;
			int end = strrchr(data[i],'"');

			config = strsub(data[i],start,(end-start));
			break;
		}
	}
	return config;
}

string setLayerFill(){
